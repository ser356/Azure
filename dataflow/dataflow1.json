{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "gobdataset",
						"type": "DatasetReference"
					},
					"name": "entradablob"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "modifiedgob_blob",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "modifiedgob_blob",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Principales indicadores} as string,",
				"          {Pertenencia a grupo empresarial} as string,",
				"          {Grupo empresarial} as string,",
				"          {Sector de actividad} as string,",
				"          {Tamaño de la empresa (según ocupados)} as string,",
				"          Periodo as string,",
				"          Total as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> entradablob",
				"entradablob derive(Subperiodo = concat(Periodo, \" Primer Semestre\")) ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy({Sector de actividad}),",
				"     SectorOrdenado = count()) ~> aggregate1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Principales indicadores} as string,",
				"          {Pertenencia a grupo empresarial} as string,",
				"          {Grupo empresarial} as string,",
				"          {Sector de actividad} as string,",
				"          {Tamaño de la empresa (según ocupados)} as string,",
				"          Periodo as string,",
				"          Total as string,",
				"          Subperiodo as string",
				"     ),",
				"     partitionFileNames:['modifiedgob.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Principales indicadores} as string,",
				"          {Pertenencia a grupo empresarial} as string,",
				"          {Grupo empresarial} as string,",
				"          {Sector de actividad} as string,",
				"          {Tamaño de la empresa (según ocupados)} as string,",
				"          Periodo as string,",
				"          Total as string,",
				"          Subperiodo as string",
				"     ),",
				"     partitionFileNames:['agregado.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink2"
			]
		}
	}
}