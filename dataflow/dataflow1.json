{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "InputCSV",
						"type": "DatasetReference"
					},
					"name": "entradablob"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "OutCSV",
						"type": "DatasetReference"
					},
					"name": "salidaMain"
				},
				{
					"dataset": {
						"referenceName": "OutCSV",
						"type": "DatasetReference"
					},
					"name": "salidaAgregado"
				}
			],
			"transformations": [
				{
					"name": "branch"
				},
				{
					"name": "agggroupbysector"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Principales indicadores} as string,",
				"          {Pertenencia a grupo empresarial} as string,",
				"          {Grupo empresarial} as string,",
				"          {Sector de actividad} as string,",
				"          {Tamaño de la empresa (según ocupados)} as string,",
				"          Periodo as string,",
				"          Total as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> entradablob",
				"entradablob derive(Subperiodo = concat(Periodo, \" Primer Semestre\"),",
				"     partitionBy('hash', 1)) ~> branch",
				"branch aggregate(groupBy({Sector de actividad}),",
				"     SectorOrdenado = count()) ~> agggroupbysector",
				"branch sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['modifiedgob.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> salidaMain",
				"agggroupbysector sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['agregado.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> salidaAgregado"
			]
		}
	}
}